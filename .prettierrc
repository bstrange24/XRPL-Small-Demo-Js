{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "printWidth": 500,
  "tabWidth": 5,
  "useTabs": false,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "htmlWhitespaceSensitivity": "css",
     "endOfLine": "auto",
     "overrides": [
       {
         "files": "*.html",
         "options": {
           "parser": "html"
         }
       }
     ]
}
